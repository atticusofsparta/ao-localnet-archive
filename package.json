{
  "name": "ao-localnet",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "ao-localnet": "cli.sh"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "aos": "./aos.sh",
    "init": "node config.mjs init",
    "config:init": "node config.mjs init",
    "config:apply": "node config.mjs apply",
    "config:show": "node config.mjs show",
    "configure": "npm run config:apply && wallets/generateAll.sh && seed/download-aos-module.sh",
    "reset": "seed/reset.mjs",
    "reseed": "seed/reset.mjs && seed/seed-for-aos.sh",
    "seed": "seed/seed-for-aos.sh",
    "start": "npm run config:apply && docker compose up --detach",
    "stop": "docker compose down",
    "spawn": "./spawn.sh",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "tsx --test tests/*.test.ts",
    "test:watch": "tsx --test --watch tests/*.test.ts",
    "test:spawn": "tsx --test tests/spawn.test.ts",
    "test:module": "tsx --test tests/module.test.ts",
    "test:message": "tsx --test tests/message.test.ts",
    "test:pingpong": "tsx --test tests/pingpong.test.ts",
    "test:config": "tsx --test tests/config.test.ts",
    "test:ratelimit": "tsx --test tests/ratelimit.test.ts"
  },
  "dependencies": {
    "@permaweb/aoconnect": "^0.0.59",
    "@permaweb/aos": "https://get_ao.g8way.io",
    "@types/dockerode": "^3.3.47",
    "arweave": "^1.15.7",
    "dockerode": "^4.0.9"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2"
  }
}
