{
  "version": "1.0",
  "description": "AO Localnet Configuration - Custom Example",
  "aos": {
    "module": "https://get_ao.g8way.io"
  },
  "wallets": {
    "directory": "./my-wallets",
    "aoWallet": "./my-wallets/ao-wallet.json",
    "bundlerWallet": "./my-wallets/bundler-wallet.json"
  },
  "data": {
    "arlocal": "./my-project/.ao-localnet/arlocal",
    "cu": "./my-project/.ao-localnet/cu",
    "mu": "./my-project/.ao-localnet/mu",
    "su": "./my-project/.ao-localnet/su",
    "suDatabase": "su-database-volume",
    "bundler": "./my-project/.ao-localnet/bundler"
  },
  "ports": {
    "arlocal": 5000,
    "mu": 5002,
    "su": 5003,
    "cu": 5004,
    "scar": 5006,
    "bundler": 5007,
    "lunar": 5008
  },
  "services": {
    "arlocal": {
      "enabled": true,
      "persist": true
    },
    "mu": {
      "enabled": true,
      "rateLimit": {
        "maxRequests": 10000,
        "intervalMs": 3600000
      }
    },
    "su": {
      "enabled": true,
      "database": {
        "user": "su_user",
        "password": "su_pass",
        "database": "su_db"
      }
    },
    "cu": {
      "enabled": true,
      "supportedModuleFormats": [
        "wasm32-unknown-emscripten",
        "wasm32-unknown-emscripten2",
        "wasm32-unknown-emscripten3",
        "wasm32-unknown-emscripten4",
        "wasm64-unknown-emscripten-draft_2024_02_15",
        "wasm32-unknown-emscripten-metering",
        "wasm64-unknown-emscripten-draft_2024_10_16-metering"
      ],
      "limits": {
        "maxMemory": "1073741824",
        "maxCompute": "9000000000000"
      }
    },
    "scar": {
      "enabled": false
    },
    "bundler": {
      "enabled": true
    },
    "lunar": {
      "enabled": false
    }
  },
  "urls": {
    "gateway": "http://localhost:5000",
    "graphql": "http://localhost:5000/graphql",
    "mu": "http://localhost:5002",
    "su": "http://localhost:5003",
    "cu": "http://localhost:5004",
    "bundler": "http://localhost:5007"
  }
}

